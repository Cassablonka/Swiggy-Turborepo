{
    "version": 2,
    "builds": [
      {
        "src": "packages/**/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "installCommand": "pnpm install --frozen-lockfile",
    "buildCommand": "pnpm turbo run build",
    "cleanUrls": true,
    "outputDirectory": "packages/app/dist",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  