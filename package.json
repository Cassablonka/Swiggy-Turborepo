{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-custom": "workspace:*",
    "lint-staged": "^13.1.2",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "husky": "^8.0.0"
  },
  "packageManager": "pnpm@7.15.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.{ts,tsx,css,scss}": [
      "prettier --write"
    ]
  }
}
