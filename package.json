{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.5.1",
    "react-toastify": "^9.1.3",
    "turbo": "^2.1.2",
    "typescript": "^4.5.4"
  },
  "packageManager": "pnpm@7.15.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.{ts,tsx,css,scss}": [
      "prettier --write"
    ]
  },
  "name": "Swiggy-Turborepo"
}
